{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/registerServiceWorker.js"
    ]
  },
  "rules": {
    "no-console": false, 
    "member-access": false, 
    "jsx-no-lambda": false, 
    "jsx-boolean-value": false,
    "ordered-input": [false] 
  } 
}
